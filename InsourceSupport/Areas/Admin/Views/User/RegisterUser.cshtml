
@{
    ViewData["Title"] = "RegisterUser";
    Layout = "~/Views/Shared/_AdminDasboardLayout.cshtml";
}
<form id="register-user" class="pr-2 ml-4 pl-2" action="/admin/user/registeruser" method="post">
    <div class="row">
        <div class="col-md-12">
            <div class="form-group">
                <div id="UserName"></div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group">
                <div id="Email"></div>
            </div>
        </div> 
        <div class="col-md-12">
            <div class="form-group">
                <div id="Password"></div>
            </div>
        </div> 
        <div class="col-md-12">
            <div class="form-group">
                <div id="FullName"></div>
            </div>
        </div> 
        <div class="col-md-12">
            <div class="form-group">
                <div id="ContactNumber"></div>
            </div>
        </div> 
        <div class="col-md-12">
            <div class="form-group">
                <div id="RoleId"></div>
            </div>
        </div> 
        <div class="col-md-12">
            <div class="form-group">
                <div id="IsSystemUser"></div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="form-group">
                <div id="registerUserSubmit"></div>
            </div>
        </div>
    </div>
</form>
@section Scripts{
    <script src="/js/custom/register-user.js"></script>
}
